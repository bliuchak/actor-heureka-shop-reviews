{
    "title": "CheerioCrawler Template",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "shopUrls": {
            "title": "Shop URLs",
            "type": "array",
            "description": "Shop URLs with reviews.",
            "editor": "stringList",
            "prefill": ["https://obchody.heureka.cz/allegro-cz/recenze/overene"]
        },
        "maxRequestsPerCrawl": {
            "title": "Max Requests per Crawl",
            "type": "integer",
            "description": "Maximum number of requests that can be made by this crawler.",
            "default": 10
        }
    }
}
